    <script>
        var item;
        $.ajax({
            type: 'GET',
            url: "/Products/GetItem",
            async: false,
            success: function (res) {
                item = res;
            }
        })
        const autoCompleteJS = new autoComplete({
            placeHolder: "Nhập tên sản phẩm...",
            data: {
                src: item,
                keys: ['Name'],
                cache: true,
            },
            resultItem: {
                highlight: true
            },
            events: {
                input: {
                    selection: (event) => {
                        debugger;
                        window.location.href = "/chi-tiet/" + event.detail.selection.value.Slug + "-" + event.detail.selection.value.Id;

                    }
                }
            }
        });
    </script>
